{
	"name": "@liguelead/n8n-nodes-liguelead",
	"version": "0.0.6",
	"private": false,
	"description": "Community node for n8n to send SMS and voice calls using the LigueLead API",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ligue-lead-tech/n8n"
	},
	"homepage": "https://github.com/ligue-lead-tech/n8n#readme",
	"keywords": [
		"n8n-community-node-package",
		"n8n-nodes",
		"liguelead",
		"sms",
		"voice",
		"telephony"
	],
	"author": {
		"name": "Henrique Corraro",
		"email": "henriquecorraro@liguelead.com.br"
	},
	"scripts": {
		"build": "n8n-node build && mkdir -p dist/nodes/LigueLead dist/credentials && cp nodes/LigueLead/logo.svg dist/nodes/LigueLead/logo.svg && cp credentials/logo.svg dist/credentials/logo.svg",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist",
		"src/nodes/**/logo.svg"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": true,
		"credentials": [
			"dist/credentials/LlApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/LigueLead/LigueLead.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"cpx": "^1.2.1",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"overrides": {
		"change-case": "4.1.2"
	}
}
